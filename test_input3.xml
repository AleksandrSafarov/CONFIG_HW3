<config>
    <comment>
        Параметры автомобиля
    </comment>

    <const name="fuelcapacity" value="75"/>
    <const name="enginepower" value="60"/>
    
    <dictionary>
        <entry name="transmissiontype" value="automatic"/>
        <entry name="interiormaterial" value="leather"/>
    </dictionary>
    
    <expr value="|mod fuelcapacity 10|"/>
    <expr value="|max enginepower 50 30|"/>
</config>
